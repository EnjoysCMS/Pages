{
  "name": "enjoyscms/pages",
  "description": "Module Pages for EnjoysCMS",
  "type": "ppi-module",
  "require": {
    "composer/installers": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\Modules\\Pages\\": "src/"
    }
  },
  "extra": {
    "installer-paths": {
      "src/Modules/{$name}/": [
        "type:ppi-module"
      ]
    }
  }
}
